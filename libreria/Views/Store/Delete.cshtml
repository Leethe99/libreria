@model libreria.Models.dbModels.Store

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar Tienda</h1>

<h3>¿Esta seguro que desea borrar este elemento?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
Nombre:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
           Calle:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Street)
        </dd>
        <dt class = "col-sm-2">
Codigo Postal:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Zipcode)
        </dd>
        <dt class = "col-sm-2">
     Ciudad:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CityNavigation.Id)
        </dd>
        <dt class = "col-sm-2">
       Pais:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CountryNavigation.Id)
        </dd>
        <dt class = "col-sm-2">
    Estado:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.State.Id)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Borrar" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-secondary">Volver a la lista</a>
    </form>
</div>
